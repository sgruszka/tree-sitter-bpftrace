{
  "name": "tree-sitter-bpftrace",
  "version": "0.1.0",
  "description": "tree-sitter parser for bpftrace",
  "repository": "https://github.com/sgruszka/tree-sitter-bpftrace",
  "funding": "https://github.com/sgruszka/tree-sitter-bpftrace",
  "license": "MIT",
  "author": {
    "name": "Stanislaw Gruszka",
    "email": "stf_xl@wp.pl"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "bpftrace"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "eslint": "^9.39.1",
    "eslint-config-treesitter": "^1.0.2",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.26.3"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "lint": "eslint grammar.js",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  },
  "tree-sitter": [
    {
      "scope": "source.bpftrace",
      "injection-regex": "^bpftrace$"
    }
  ]
}
